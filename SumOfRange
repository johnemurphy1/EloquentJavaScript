//Assignment: write range function that takes 2 arguments, start and end, and returns an array containing all numbers from start up to (and including) end. 
//Next, write sum function that takes array of numbers and returns sum of these numbers.
let range = [];
let sum = 0;//declare and initialize sum variable
const start = 0;//declare start variable as constant
const end = 10;//declare end variable as constant

function addToRange(start,end){
  for(let i=start; i <= end; i++){//use start and end parameters
    range.push(i);//push current value of i into range array
    sum += i;//add current value of i to sum
  }
  return range;
}
addToRange(start,end);//call function
console.log(range);
console.log(sum);
